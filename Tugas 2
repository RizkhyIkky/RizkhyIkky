package struktur;

import java.util.Arrays;
import javax.sound.sampled.DataLine;

public class ikky {
    public static void main(String[] args) {
        // 5 metode mencetak isi array
        int[] angka = {50, 60, 70, 80, 70};
        
        // menggunakan loop for
        for (int i = 0; i < angka.length; i++) {
            System.out.println(angka[i]);
            
        }
        // menggunakan for each
        for (int data : angka) {
            System.out.println(angka);
        }
        // menggunakan metode Arrays. toString() 
        System.out.println(Arrays.toString(angka));
        
        // menggunakan metode Arrays.asList()
        System.out.println(Arrays.asList(angka));
        
        // menggunakan metode java 8 Stream API
        Arrays.stream(angka). forEach(System.out::println);
    }
    
}
